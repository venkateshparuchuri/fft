        <div class="row staff-container">
            <div class="col-md-6">
                <div class="col-md-12 module-layout">
                    <h4 class="heading-class">Weekly Staff Statistics</h4>

                    <div id="weelystaffbar" style="min-height:200px;margin-top:0px;"></div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="col-md-12 module-layout">
                    <h4 class="heading-class">Weekly Students Statistics</h4>

                    <div id="weeklystudentsbar" style="min-height:200px;margin-top:0px;"></div>
